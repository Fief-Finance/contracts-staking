{
  "name": "@fief-finance/contracts-staking",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Staking Smart Contracts for Fief Finance Guild",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-vyper": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/test-helpers": "^0.5.12",
    "@solidity-parser/parser": "^0.14.0",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^9.0.0",
    "@types/mocha-each": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "bignumber.js": "^9.0.1",
    "chai": "^4.3.4",
    "commander": "^9.1.0",
    "dotenv": "^10.0.0",
    "erc721a": "^3.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.5.4",
    "hardhat": "2.8.3",
    "hardhat-gas-reporter": "^1.0.7",
    "lodash": "^4.17.20",
    "mem": "4.0.0",
    "mocha": "^9.1.3",
    "mocha-each": "^2.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.18",
    "truffle-assertions": "^0.9.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "test": "npx hardhat test",
    "test:coverage": "npx hardhat coverage",
    "compile": "npx hardhat compile --no-typechain && npx hardhat typechain",
    "build": "npm run compile",
    "typechain": "hardhat typechain",
    "clean": "npx hardhat clean",
    "lint:sol": "solhint -f table contracts/**/*.sol",
    "format:sol": "prettier --write 'contracts/**/*.sol'",
    "format:ts": "prettier --write '{scripts,src,tasks,test}/**/*.ts'",
    "format": "yarn format:sol && yarn format:ts",
    "1-deploy-mock-token:goerli": "npx hardhat 1-deploy-mock-token --network goerli",
    "2-deploy-sfief:goerli": "npx hardhat deploy-sfief --network goerli",
    "3-deploy-distributor:goerli": "npx hardhat deploy-distributor --network goerli",
    "4-sfief-checkpoint:goerli": "npx hardhat 4-sfief-checkpoint --network goerli",
    "5-fee-distributor-checkpoint-token:goerli": "npx hardhat 5-fee-distributor-checkpoint-token --network goerli",
    "1-deploy-mock-token:ethereum": "npx hardhat 1-deploy-mock-token --network ethereum",
    "2-deploy-sfief:ethereum": "npx hardhat deploy-sfief --network ethereum",
    "3-deploy-distributor:ethereum": "npx hardhat deploy-distributor --network ethereum",
    "4-sfief-checkpoint:ethereum": "npx hardhat 4-sfief-checkpoint --network ethereum",
    "5-fee-distributor-checkpoint-token:ethereum": "npx hardhat 5-fee-distributor-checkpoint-token --network ethereum"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fief-Finance/contracts-staking.git"
  },
  "author": "FIEF Finance",
  "bugs": {
    "url": "https://github.com/Fief-Finance/contracts-staking/issues"
  },
  "homepage": "https://github.com/Fief-Finance/contracts-staking#readme"
}
